<?xml version="1.0" encoding="UTF-8"?>
<!-- 
	登录按钮的圆角
 -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle" >

    <!-- 填充的颜色 -->
    <solid android:color="@color/unitform_button_gray" />
    <!-- 线的颜色 -->
   <!--  <stroke  
       android:width="1dip"  
       android:color="@color/default_line_color" /> -->
    <!-- 设置按钮的四个角为弧形 -->
    <!-- android:radius 弧形的半径 -->
    <corners android:radius="5dip" />

    <!-- padding：Button里面的文字与Button边界的间隔 -->
    <padding
        android:bottom="7dip"
        android:left="7dip"
        android:right="7dip"
        android:top="7dip" />
</shape>